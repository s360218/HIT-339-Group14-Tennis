@model tennis.Models.Schedule

<h2>Enrolled Members for @Model.Name (@Model.Date.ToShortDateString())</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var memberSchedule in Model.MemberSchedules)
        {
                <tr>
                    <td>@memberSchedule.Member.FirstName</td>
                    <td>@memberSchedule.Member.LastName</td>
                    <td>@memberSchedule.Member.Email</td>
                </tr>
        }
    </tbody>
</table>

<div>
    <a asp-action="MySchedules">Back to My Schedules</a>
</div>
